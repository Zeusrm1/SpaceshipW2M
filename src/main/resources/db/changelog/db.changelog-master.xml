    <changeSet id="1" author="zrodriguez">
        <createTable tableName="TBL_SPACESHIPS">
            <column name="id" type="INT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="spaceship_name" type="VARCHAR(250)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="2" author="zrodriguez">
